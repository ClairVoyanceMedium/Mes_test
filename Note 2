        script.textContent = JSON.stringify([structuredData, productData]);
        document.head.appendChild(script);

        // Gestion des erreurs de chargement des ressources
        window.addEventListener('error', (e) => {
            if (e.target.tagName === 'IMG') {
                console.warn('Erreur de chargement de l\'image:', e.target.src);
                e.target.src = '/fallback-image.webp'; // Image de secours
            }
        }, true);

        // Lazy loading des images non critiques
        document.querySelectorAll('img[loading="lazy"]').forEach(img => {
            img.setAttribute('decoding', 'async');
        });

        // Gestion des carrousels améliorée
        function setupCarousel(carouselId) {
            const carousel = document.getElementById(carouselId);
            if (!carousel) return;

            const inner = carousel.querySelector('.carousel-inner');
            const items = carousel.querySelectorAll('.carousel-item');
            const prevBtn = carousel.querySelector('.prev');
            const nextBtn = carousel.querySelector('.next');
            let currentIndex = 0;

            function updateCarousel() {
                items.forEach((item, index) => {
                    item.setAttribute('aria-hidden', index !== currentIndex);
                    item.tabIndex = index === currentIndex ? 0 : -1;
                    item.classList.toggle('active', index === currentIndex);
                    
                    if (index === currentIndex) {
                        item.style.transform = 'translateX(0)';
                        item.style.opacity = '1';
                    } else if (index < currentIndex) {
                        item.style.transform = 'translateX(-100%)';
                        item.style.opacity = '0';
                    } else {
                        item.style.transform = 'translateX(100%)';
                        item.style.opacity = '0';
                    }
                });

                // Mise à jour de l'ARIA
                carousel.setAttribute('aria-label', `${carouselId.replace('Carousel', '')} carrousel, slide ${currentIndex + 1} sur ${items.length}`);
            }

            function nextSlide() {
                currentIndex = (currentIndex + 1) % items.length;
                updateCarousel();
            }

            function prevSlide() {
                currentIndex = (currentIndex - 1 + items.length) % items.length;
                updateCarousel();
            }

            if (prevBtn) prevBtn.addEventListener('click', prevSlide);
            if (nextBtn) nextBtn.addEventListener('click', nextSlide);

            // Navigation au clavier
            carousel.addEventListener('keydown', (e) => {
                if (e.key === 'ArrowRight') nextSlide();
                if (e.key === 'ArrowLeft') prevSlide();
            });

            // Initialisation
            updateCarousel();
        }

        // Initialisation des carrousels
        setupCarousel('avisCarousel');
        setupCarousel('blogCarousel');
        setupCarousel('david-carousel');

        // Gestion des cookies pour la vidéo YouTube
        const cookieConsent = document.getElementById('cookieConsent');
        const acceptCookies = document.getElementById('acceptCookies');
        const rejectCookies = document.getElementById('rejectCookies');
        const videoContainer = document.querySelector('.video-container iframe');

        if (!localStorage.getItem('cookieConsent')) {
            cookieConsent.style.display = 'block';
        }

        acceptCookies.addEventListener('click', () => {
            localStorage.setItem('cookieConsent', 'accepted');
            cookieConsent.style.display = 'none';
            // Recharger la vidéo si nécessaire
            if (videoContainer) {
                const src = videoContainer.src;
                videoContainer.src = '';
                videoContainer.src = src;
            }
        });

        rejectCookies.addEventListener('click', () => {
            localStorage.setItem('cookieConsent', 'rejected');
            cookieConsent.style.display = 'none';
            document.getElementById('video-error').style.display = 'block';
        });

        // Gestion du son de la vidéo YouTube
        const muteToggle = document.querySelector('.mute-toggle');
        if (muteToggle) {
            muteToggle.addEventListener('click', function() {
                const iframe = document.querySelector('.video-container iframe');
                if (iframe) {
                    const muteParam = iframe.src.includes('mute=1') ? '0' : '1';
                    iframe.src = iframe.src.replace(/mute=[01]/, `mute=${muteParam}`);
                    this.textContent = muteParam === '1' ? 'Activer le son' : 'Désactiver le son';
                }
            });
        }
    </script>

    <!-- Manifest pour PWA -->
    <link rel="manifest" href="/manifest.json">
    <script nonce="xai-premium2025">
        if ('serviceWorker' in navigator) {
            window.addEventListener('load', () => {
                navigator.serviceWorker.register('/sw.js').then(registration => {
                    console.log('Service Worker enregistré:', registration);
                }).catch(error => {
                    console.error('Erreur d\'enregistrement du Service Worker:', error);
                });
            });
        }
    </script>
</body>
</html>


