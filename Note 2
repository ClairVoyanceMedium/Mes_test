  <script async src="https://www.googletagmanager.com/gtag/js?id=AW-CONVERSION_ID"></script>
  <script nonce="48273">
    (function() {
      const initGoogleAds = ({ conversionId, conversionLabel = null, transactionValue = null, currency = 'EUR' }) => {
        window.dataLayer = window.dataLayer || [];
        function gtag(){ dataLayer.push(arguments); }
        gtag('js', new Date());
        gtag('config', `AW-${conversionId}`);
        gtag('event', 'page_view', { 'send_to': `AW-${conversionId}` });
        if (conversionLabel && transactionValue !== null) {
          gtag('event', 'conversion', {
            'send_to': `AW-${conversionId}/${conversionLabel}`,
            'value': transactionValue,
            'currency': currency
          });
        }
      };
      const googleAdsConfig = {
        conversionId: 'AW-00000000000',
        conversionLabel: 'CONVERSION_LABEL_OPTIONNEL',
        transactionValue: 0
      };
      initGoogleAds(googleAdsConfig);
    })();
  </script>

