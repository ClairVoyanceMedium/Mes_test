  <script defer>
    // Mise à jour dynamique :
    // Pour la phrase sous le logo ("comment gagner au loto en [mois année]")
    const now = new Date();
    const currentYear = now.getFullYear();
    const currentMonth = now.toLocaleString('fr-FR', { month: 'long' });
    // Mise à jour pour la phrase stylisée sous le logo
    document.getElementById("dynamicDate").textContent = currentMonth + " " + currentYear;
