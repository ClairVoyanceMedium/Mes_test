        // Fonction pour initialiser Google Translate
        function googleTranslateElementInit() {
            new google.translate.TranslateElement({
                pageLanguage: 'fr',
                includedLanguages: 'en,es,de,it,pt,ru,zh-CN,ja,ar,hi,fr,pl,nl,sv,da,fi,no,cs,hu,ro,sk,sl,hr,bg,el,et,lv,lt,ga,mt,tr,th,vi,ko,bn,ms,sw',
                layout: google.translate.TranslateElement.InlineLayout.SIMPLE,
                autoDisplay: false
            }, 'google_translate_element');
        }

        // Fonction pour afficher la popup de sortie
        function showExitPopup() {
            if (!sessionStorage.getItem('popupShown')) {
                const exitIntentPopup = document.getElementById('exitIntentPopup');
                exitIntentPopup.classList.add('active');
                exitIntentPopup.setAttribute('aria-hidden', 'false');
                sessionStorage.setItem('popupShown', 'true');
            }
        }

        // D√©tection de sortie de la page
        document.addEventListener('mouseout', (e) => {
            if (e.clientY <= 0 && !document.getElementById('exitIntentPopup').classList.contains('active')) {
                showExitPopup();
            }
        });

        // Gestion des boutons audio am√©lior√©e
        const audioButtons = document.querySelectorAll('.audio-button');
        let currentAudio = null;

        audioButtons.forEach(button => {
            button.addEventListener('click', () => {
                const textIds = button.dataset.textId.split(',');
                let textContent = '';

                textIds.forEach(id => {
                    const element = document.getElementById(id) || document.querySelector(id);
                    if (element) {
                        if (element.classList.contains('carousel-inner')) {
                            // Pour les carrousels, lire uniquement l'√©l√©ment actif
                            const activeItem = element.querySelector('.carousel-item.active');
                            if (activeItem) {
                                textContent += activeItem.innerText + ' ';
                            }
                        } else {
                            textContent += element.innerText + ' ';
                        }
                    }
                });

                if (currentAudio) {
                    currentAudio.pause();
                    currentAudio = null;
                    button.textContent = '√âcouter';
                    button.setAttribute('aria-label', '√âcouter le texte');
                    return;
                }

                if ('speechSynthesis' in window) {
                    const utterance = new SpeechSynthesisUtterance(textContent);
                    utterance.lang = 'fr-FR';
                    utterance.rate = 1.0;
                    utterance.pitch = 1.0;
                    utterance.volume = 1.0;

                    utterance.onend = () => {
                        button.textContent = '√âcouter';
                        button.setAttribute('aria-label', '√âcouter le texte');
                        currentAudio = null;
                    };

                    speechSynthesis.speak(utterance);
                    currentAudio = utterance;
                    button.textContent = 'Arr√™ter';
                    button.setAttribute('aria-label', 'Arr√™ter la lecture audio');
                } else {
                    alert('La synth√®se vocale n\'est pas prise en charge par votre navigateur.');
                }
            });
        });

        // Mise √† jour de l'ann√©e dans le footer et le header
        const currentYear = new Date().getFullYear();
        document.getElementById('current-year').textContent = currentYear;
        document.getElementById('footer-year').textContent = currentYear;

        // Conversion des prix selon la devise
        const priceElements = document.querySelectorAll('.price');
        let userCurrency = 'EUR';

        async function fetchExchangeRates() {
            try {
                const response = await fetch('https://api.exchangerate-api.com/v4/latest/EUR');
                const data = await response.json();
                return data.rates;
            } catch (error) {
                console.error('Erreur lors de la r√©cup√©ration des taux de change:', error);
                return null;
            }
        }

        async function updatePrices() {
            const rates = await fetchExchangeRates();
            if (!rates) return;

            // D√©tection de la devise en fonction de la localisation
            try {
                const response = await fetch('https://ipapi.co/json/');
                const data = await response.json();
                const country = data.country_code;
                userCurrency = country === 'US' ? 'USD' : country === 'GB' ? 'GBP' : 'EUR';
            } catch (error) {
                console.error('Erreur lors de la d√©tection de la localisation:', error);
            }

            priceElements.forEach(element => {
                const eurPrice = parseFloat(element.dataset.eurPrice);
                let convertedPrice = eurPrice;

                if (userCurrency !== 'EUR' && rates[userCurrency]) {
                    convertedPrice = (eurPrice * rates[userCurrency]).toFixed(2);
                }

                element.textContent = `${convertedPrice} ${userCurrency}`;
            });
        }

        window.addEventListener('load', updatePrices);

        // Gestion de l'installation de l'application (PWA)
        let deferredPrompt = null;
        const installButton = document.querySelector('.install-button');

        window.addEventListener('beforeinstallprompt', (e) => {
            e.preventDefault();
            deferredPrompt = e;
            installButton.style.display = 'inline-block';
        });

        installButton.addEventListener('click', async () => {
            if (deferredPrompt) {
                deferredPrompt.prompt();
                const { outcome } = await deferredPrompt.userChoice;
                if (outcome === 'accepted') {
                    console.log('Application install√©e');
                } else {
                    console.log('Installation refus√©e');
                }
                deferredPrompt = null;
                installButton.style.display = 'none';
            }
        });

        window.addEventListener('appinstalled', () => {
            console.log('PWA install√©e avec succ√®s');
            installButton.style.display = 'none';
        });

        // Google Analytics avec tracking am√©lior√©
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());
        gtag('config', 'G-2RSFE6WL4M');

        // Tracking des boutons
        document.querySelectorAll('[data-tracking-label]').forEach(button => {
            button.addEventListener('click', function() {
                gtag('event', 'click', {
                    'event_category': 'Button',
                    'event_label': this.dataset.trackingLabel
                });
            });
        });

        // Web Vitals pour optimiser les performances
        webVitals.getCLS((metric) => console.log('CLS:', metric.value));
        webVitals.getFID((metric) => console.log('FID:', metric.value));
        webVitals.getLCP((metric) => console.log('LCP:', metric.value));

        // Pr√©chargement des ressources critiques
        document.addEventListener('DOMContentLoaded', () => {
            const criticalResources = [
                '/fonts/Cinzel-Regular.woff2',
                'https://i.imgur.com/GmyGlKk.webp'
            ];
            criticalResources.forEach(resource => {
                const link = document.createElement('link');
                link.rel = 'preload';
                link.as = resource.includes('.woff2') ? 'font' : 'image';
                link.href = resource;
                link.crossOrigin = resource.includes('woff2') ? 'anonymous' : undefined;
                document.head.appendChild(link);
            });
        });

        // Gestion du d√©filement fluide pour les ancres
        document.querySelectorAll('a[href^="#"]').forEach(anchor => {
            anchor.addEventListener('click', (e) => {
                e.preventDefault();
                const targetId = anchor.getAttribute('href').substring(1);
                const targetElement = document.getElementById(targetId);
                if (targetElement) {
                    targetElement.scrollIntoView({ behavior: 'smooth' });
                }
            });
        });

        // Accessibilit√© : navigation au clavier
        document.addEventListener('keydown', (e) => {
            if (e.key === 'Escape' && exitIntentPopup.classList.contains('active')) {
                exitIntentPopup.classList.remove('active');
                exitIntentPopup.setAttribute('aria-hidden', 'true');
            }
        });

        // Optimisation SEO : donn√©es structur√©es
        const structuredData = {
            "@context": "https://schema.org",
            "@type": "WebSite",
            "name": "ClairvoyanceMedium.com",
            "url": "https://clairvoyancemedium.github.io/voyance-minceur.github.io/",
            "potentialAction": {
                "@type": "SearchAction",
                "target": "https://clairvoyancemedium.github.io/voyance-minceur.github.io/search?q={search_term_string}",
                "query-input": "required name=search_term_string"
            },
            "publisher": {
                "@type": "Organization",
                "name": "ClairvoyanceMedium.com",
                "logo": {
                    "@type": "ImageObject",
                    "url": "https://i.imgur.com/GmyGlKk.webp"
                }
            }
        };

        const productData = {
            "@context": "https://schema.org",
            "@type": "Product",
            "name": "G√©lules Minceur Victory",
            "image": "https://i.imgur.com/GmyGlKk.webp",
            "description": "G√©lules minceur Victory pour perdre du poids rapidement et naturellement, jusqu'√† 12 kg en 2 mois.",
            "brand": {
                "@type": "Brand",
                "name": "ClairvoyanceMedium.com"
            },
            "offers": {
                "@type": "Offer",
                "url": "https://buy.stripe.com/9AQ28w5Zag1p5KE29u",
                "priceCurrency": "EUR",
                "price": "19.00",
                "availability": "https://schema.org/InStock",
                "seller": {
                    "@type": "Organization",
                    "name": "ClairvoyanceMedium.com"
                }
            },
            "aggregateRating": {
                "@type": "AggregateRating",
                "ratingValue": "4.9",
                "reviewCount": "9000"
            }
        };

        const script = document.createElement('script');
        script.type = 'application/ld+json';
        script.textContent = JSON.stringify([structuredData, productData]);
        document.head.appendChild(script);

        // Gestion des erreurs de chargement des ressources
        window.addEventListener('error', (e) => {
            if (e.target.tagName === 'IMG') {
                console.warn('Erreur de chargement de l\'image:', e.target.src);
                e.target.src = '/fallback-image.webp'; // Image de secours
            }
        }, true);

        // Lazy loading des images non critiques
        document.querySelectorAll('img[loading="lazy"]').forEach(img => {
            img.setAttribute('decoding', 'async');
        });

        // Gestion des carrousels am√©lior√©e
        function setupCarousel(carouselId) {
            const carousel = document.getElementById(carouselId);
            if (!carousel) return;

            const inner = carousel.querySelector('.carousel-inner');
            const items = carousel.querySelectorAll('.carousel-item');
            const prevBtn = carousel.querySelector('.prev');
            const nextBtn = carousel.querySelector('.next');
            let currentIndex = 0;

            function updateCarousel() {
                items.forEach((item, index) => {
                    item.setAttribute('aria-hidden', index !== currentIndex);
                    item.tabIndex = index === currentIndex ? 0 : -1;
                    item.classList.toggle('active', index === currentIndex);
                    
                    if (index === currentIndex) {
                        item.style.transform = 'translateX(0)';
                        item.style.opacity = '1';
                    } else if (index < currentIndex) {
                        item.style.transform = 'translateX(-100%)';
                        item.style.opacity = '0';
                    } else {
                        item.style.transform = 'translateX(100%)';
                        item.style.opacity = '0';
                    }
                });

                // Mise √† jour de l'ARIA
                carousel.setAttribute('aria-label', `${carouselId.replace('Carousel', '')} carrousel, slide ${currentIndex + 1} sur ${items.length}`);
            }

            function nextSlide() {
                currentIndex = (currentIndex + 1) % items.length;
                updateCarousel();
            }

            function prevSlide() {
                currentIndex = (currentIndex - 1 + items.length) % items.length;
                updateCarousel();
            }

            if (prevBtn) prevBtn.addEventListener('click', prevSlide);
            if (nextBtn) nextBtn.addEventListener('click', nextSlide);

            // Navigation au clavier
            carousel.addEventListener('keydown', (e) => {
                if (e.key === 'ArrowRight') nextSlide();
                if (e.key === 'ArrowLeft') prevSlide();
            });

            // Initialisation
            updateCarousel();
        }

        // Initialisation des carrousels
        setupCarousel('avisCarousel');
        setupCarousel('blogCarousel');
        setupCarousel('david-carousel');

        // Gestion des cookies pour la vid√©o YouTube
        const cookieConsent = document.getElementById('cookieConsent');
        const acceptCookies = document.getElementById('acceptCookies');
        const rejectCookies = document.getElementById('rejectCookies');
        const videoContainer = document.querySelector('.video-container iframe');

        if (!localStorage.getItem('cookieConsent')) {
            cookieConsent.style.display = 'block';
        }

        acceptCookies.addEventListener('click', () => {
            localStorage.setItem('cookieConsent', 'accepted');
            cookieConsent.style.display = 'none';
            // Recharger la vid√©o si n√©cessaire
            if (videoContainer) {
                const src = videoContainer.src;
                videoContainer.src = '';
                videoContainer.src = src;
            }
        });

        rejectCookies.addEventListener('click', () => {
            localStorage.setItem('cookieConsent', 'rejected');
            cookieConsent.style.display = 'none';
            document.getElementById('video-error').style.display = 'block';
        });

        // Gestion du son de la vid√©o YouTube
        const muteToggle = document.querySelector('.mute-toggle');
        if (muteToggle) {
            muteToggle.addEventListener('click', function() {
                const iframe = document.querySelector('.video-container iframe');
                if (iframe) {
                    const muteParam = iframe.src.includes('mute=1') ? '0' : '1';
                    iframe.src = iframe.src.replace(/mute=[01]/, `mute=${muteParam}`);
                    this.textContent = muteParam === '1' ? 'Activer le son' : 'D√©sactiver le son';
                }
            });
        }
    </script>

    <!-- Manifest pour PWA -->
    <link rel="manifest" href="/manifest.json">
    <script nonce="xai-premium2025">
        if ('serviceWorker' in navigator) {
            window.addEventListener('load', () => {
                navigator.serviceWorker.register('/sw.js').then(registration => {
                    console.log('Service Worker enregistr√©:', registration);
                }).catch(error => {
                    console.error('Erreur d\'enregistrement du Service Worker:', error);
                });
            });
        }
    </script>
</body>
</html>


