        // Mise √† jour de l'ann√©e dans le footer et l'en-t√™te
        const currentYear = new Date().getFullYear();
        document.getElementById('current-year').textContent = currentYear;
        document.getElementById('footer-year').textContent = currentYear;

        // Initialisation de Google Translate
        function googleTranslateElementInit() {
            new google.translate.TranslateElement({
                pageLanguage: 'fr',
                includedLanguages: 'en,es,de,it,pt,nl,sv,da,fi,no,pl,cs,hu,ro,sk,sl,hr,bg,el,et,lv,lt,ga,mt,ru,zh-CN,hi,ar,bn,ja,ko,vi,th,ms,tr,sw',
                layout: google.translate.TranslateElement.InlineLayout.SIMPLE,
                autoDisplay: false
            }, 'google_translate_element');
        }

        // Gestion des cookies
        const cookieConsent = document.getElementById('cookieConsent');
        const acceptCookies = document.getElementById('acceptCookies');
        const rejectCookies = document.getElementById('rejectCookies');
        const videoIframe = document.querySelector('.video-container iframe');
        let cookiesAccepted = false;

        function checkCookieConsent() {
            if (!localStorage.getItem('cookieConsent')) {
                cookieConsent.style.display = 'block';
            } else if (localStorage.getItem('cookieConsent') === 'accepted') {
                cookiesAccepted = true;
                loadVideo();
            }
        }

        acceptCookies.addEventListener('click', () => {
            localStorage.setItem('cookieConsent', 'accepted');
            cookieConsent.style.display = 'none';
            cookiesAccepted = true;
            loadVideo();
        });

        rejectCookies.addEventListener('click', () => {
            localStorage.setItem('cookieConsent', 'rejected');
            cookieConsent.style.display = 'none';
            cookiesAccepted = false;
            document.getElementById('video-error').style.display = 'block';
        });

        function loadVideo() {
            if (cookiesAccepted && videoIframe.dataset.src) {
                videoIframe.src = videoIframe.dataset.src;
                document.getElementById('video-error').style.display = 'none';
            }
        }

        window.addEventListener('load', checkCookieConsent);

        // Gestion de la vid√©o (mute/unmute)
        const muteToggle = document.querySelector('.mute-toggle');
        muteToggle.addEventListener('click', () => {
            if (cookiesAccepted) {
                const isMuted = videoIframe.src.includes('mute=1');
                videoIframe.src = isMuted ? videoIframe.src.replace('mute=1', 'mute=0') : videoIframe.src.replace('mute=0', 'mute=1');
                muteToggle.textContent = isMuted ? 'D√©sactiver le son' : 'Activer le son';
            } else {
                document.getElementById('video-error').style.display = 'block';
            }
        });

        // Gestion du carrousel (avis et blog)
        function initCarousel(carouselId) {
            const carousel = document.getElementById(carouselId);
            const inner = carousel.querySelector('.carousel-inner');
            const items = inner.querySelectorAll('.carousel-item');
            const prevBtn = carousel.querySelector('.prev');
            const nextBtn = carousel.querySelector('.next');
            let currentIndex = 0;

            function updateCarousel() {
                items.forEach((item, index) => {
                    item.classList.remove('active');
                    item.setAttribute('aria-hidden', 'true');
                    item.setAttribute('tabindex', '-1');
                    item.style.transform = index < currentIndex ? 'translateX(-100%)' : 'translateX(100%)';
                    item.style.opacity = '0';
                });
                items[currentIndex].classList.add('active');
                items[currentIndex].setAttribute('aria-hidden', 'false');
                items[currentIndex].setAttribute('tabindex', '0');
                items[currentIndex].style.transform = 'translateX(0)';
                items[currentIndex].style.opacity = '1';
                carousel.setAttribute('aria-label', `Carrousel ${carouselId === 'avisCarousel' ? 'd‚Äôavis' : 'd‚Äôarticles de blog'}, slide ${currentIndex + 1} sur ${items.length}`);
            }

            prevBtn.addEventListener('click', () => {
                currentIndex = (currentIndex - 1 + items.length) % items.length;
                updateCarousel();
            });

            nextBtn.addEventListener('click', () => {
                currentIndex = (currentIndex + 1) % items.length;
                updateCarousel();
            });

            updateCarousel();
        }

        initCarousel('avisCarousel');
        initCarousel('blogCarousel');

        // Gestion du calculateur IMC
        const imcForm = document.getElementById('imcForm');
        const imcResult = document.getElementById('imcResult');
        const imcError = document.getElementById('imc-error');
        const imcChart = document.getElementById('imcChart').getContext('2d');
        const imcRecommendation = document.getElementById('imc-recommendation');

        let chartInstance = null;

        imcForm.addEventListener('submit', (e) => {
            e.preventDefault();
            const weight = parseFloat(document.getElementById('weight').value);
            const height = parseFloat(document.getElementById('height').value) / 100;

            if (isNaN(weight) || isNaN(height) || weight < 10 || weight > 300 || height < 0.5 || height > 2.8) {
                imcError.textContent = 'Veuillez entrer un poids (10-300 kg) et une taille (50-280 cm) valides.';
                return;
            }

            imcError.textContent = '';
            const imc = (weight / (height * height)).toFixed(1);
            let category = '';

            if (imc < 18.5) category = 'Insuffisance pond√©rale';
            else if (imc < 25) category = 'Poids normal';
            else if (imc < 30) category = 'Surpoids';
            else category = 'Ob√©sit√©';

            imcResult.innerHTML = `Votre IMC est de <strong>${imc}</strong> (${category}).`;
            imcRecommendation.style.display = 'block';
            imcRecommendation.setAttribute('aria-hidden', 'false');

            if (chartInstance) chartInstance.destroy();

            chartInstance = new Chart(imcChart, {
                type: 'bar',
                data: {
                    labels: ['Votre IMC', 'Insuffisance', 'Normal', 'Surpoids', 'Ob√©sit√©'],
                    datasets: [{
                        label: 'IMC',
                        data: [imc, 18.5, 25, 30, 40],
                        backgroundColor: ['#87CEEB', '#2ecc71', '#2ecc71', '#ff6b6b', '#ff6b6b'],
                        borderColor: ['#fff', '#fff', '#fff', '#fff', '#fff'],
                        borderWidth: 1
                    }]
                },
                options: {
                    responsive: true,
                    maintainAspectRatio: false,
                    scales: {
                        y: { beginAtZero: true, max: 50 }
                    },
                    plugins: {
                        legend: { display: false },
                        title: { display: true, text: 'Votre IMC en Contexte' }
                    }
                }
            });
        });

        // Gestion de la newsletter
        const subscribeForm = document.getElementById('subscribeForm');
        const subscribeResult = document.getElementById('subscribe-result');

        subscribeForm.addEventListener('submit', async (e) => {
            e.preventDefault();
            const email = document.getElementById('email').value;

            if (!email.match(/^[^\s@]+@[^\s@]+\.[^\s@]+$/)) {
                subscribeResult.textContent = 'Veuillez entrer un email valide.';
                return;
            }

            try {
                // Simulation d‚Äôappel API (remplacer par votre endpoint r√©el)
                const response = await fetch('/api/subscribe', {
                    method: 'POST',
                    headers: { 'Content-Type': 'application/json' },
                    body: JSON.stringify({ email })
                });

                if (response.ok) {
                    subscribeResult.textContent = 'Inscription r√©ussie ! V√©rifiez votre email pour confirmer.';
                    subscribeForm.reset();
                } else {
                    subscribeResult.textContent = 'Erreur lors de l‚Äôinscription. Veuillez r√©essayer.';
                }
            } catch (error) {
                subscribeResult.textContent = 'Erreur r√©seau. Veuillez r√©essayer plus tard.';
            }
        });

        // Gestion de la popup d‚Äôintention de sortie
        const exitIntentPopup = document.getElementById('exitIntentPopup');
        const popupCloseButton = exitIntentPopup.querySelector('.popup-close-button');

        function showExitIntentPopup() {
            if (!sessionStorage.getItem('popupShown')) {
                exitIntentPopup.classList.add('active');
                exitIntentPopup.setAttribute('aria-hidden', 'false');
                sessionStorage.setItem('popupShown', 'true');
            }
        }

        popupCloseButton.addEventListener('click', () => {
            exitIntentPopup.classList.remove('active');
            exitIntentPopup.setAttribute('aria-hidden', 'true');
        });

        document.addEventListener('mouseout', (e) => {
            if (e.clientY < 0 && !exitIntentPopup.classList.contains('active')) {
                showExitIntentPopup();
            }
        });

        // D√©tection de langue et devise
        async function detectLanguageAndCurrency() {
            try {
                const response = await fetch('https://ipapi.co/json/');
                const data = await response.json();
                const userCountry = data.country_code;
                const userCurrency = {
                    'US': 'USD', 'GB': 'GBP', 'CA': 'CAD', 'AU': 'AUD', 'IN': 'INR', 'JP': 'JPY',
                    'CN': 'CNY', 'RU': 'RUB', 'BR': 'BRL', 'ZA': 'ZAR', 'default': 'EUR'
                }[userCountry] || 'EUR';

                // Mise √† jour des prix en fonction de la devise
                const priceElements = document.querySelectorAll('.prix');
                priceElements.forEach(async (el) => {
                    const eurPrice = parseFloat(el.dataset.eurPrice);
                    if (userCurrency !== 'EUR') {
                        try {
                            const rateResponse = await fetch(`https://api.exchangerate-api.com/v4/latest/EUR`);
                            const rateData = await rateResponse.json();
                            const rate = rateData.rates[userCurrency];
                            const convertedPrice = (eurPrice * rate).toFixed(2);
                            el.querySelector('.price').textContent = `${convertedPrice} ${userCurrency}`;
                        } catch (error) {
                            console.error('Erreur lors de la conversion de devise:', error);
                        }
                    }
                });

                // Suggestion de langue
                const userLanguage = data.languages.split(',')[0].split('-')[0];
                if (userLanguage !== 'fr' && document.getElementById('google_translate_element')) {
                    const translateSelect = document.querySelector('.goog-te-combo');
                    if (translateSelect) {
                        translateSelect.value = userLanguage;
                        translateSelect.dispatchEvent(new Event('change'));
                    }
                }
            } catch (error) {
                console.error('Erreur lors de la d√©tection de localisation:', error);
            }
        }

        window.addEventListener('load', detectLanguageAndCurrency);

        // Gestion des boutons audio
        const audioButtons = document.querySelectorAll('.audio-button');
        let currentAudio = null;

        audioButtons.forEach(button => {
            button.addEventListener('click', async () => {
                const textIds = button.dataset.textId.split(',');
                const textElements = textIds.map(id => document.getElementById(id) || document.querySelector(id));
                const text = textElements.map(el => el?.innerText || '').join(' ').trim();

                if (!text) return;

                if (currentAudio) {
                    currentAudio.pause();
                    currentAudio = null;
                }

                try {
                    const response = await fetch('https://api.streamelements.com/kappa/v2/speech', {
                        method: 'POST',
                        headers: { 'Content-Type': 'application/json' },
                        body: JSON.stringify({ text, voice: 'fr-FR-Wavenet-A' })
                    });

                    if (response.ok) {
                        const audioBlob = await response.blob();
                        const audioUrl = URL.createObjectURL(audioBlob);
                        currentAudio = new Audio(audioUrl);
                        currentAudio.play();
                        currentAudio.onended = () => URL.revokeObjectURL(audioUrl);
                    } else {
                        alert('Erreur lors de la g√©n√©ration de l‚Äôaudio.');
                    }
                } catch (error) {
                    console.error('Erreur audio:', error);
                    alert('Erreur r√©seau. Veuillez r√©essayer.');
                }
            });
        });

