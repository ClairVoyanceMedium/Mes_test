    // Fonction de suivi pour tous les boutons et liens
    function trackClick(eventLabel) {
      console.log(`Tracking: ${eventLabel}`);
      if (typeof gtag === 'function') {
        gtag('event', 'click', { event_category: 'Interaction', event_label: eventLabel, value: 1 });
      }
    }

    // Données Loto simulées
    const lotoData = {
      history: [
        { numbers: new Uint8Array([5, 12, 19, 26, 33]), chance: 8 },
        { numbers: new Uint8Array([7, 13, 22, 41, 38]), chance: 7 },
        { numbers: new Uint8Array([7, 15, 22, 41, 38]), chance: 9 },
        { numbers: new Uint8Array([4, 12, 19, 41, 38]), chance: 6 },
        { numbers: new Uint8Array([5, 13, 19, 41, 33]), chance: 8 }
      ]
    };
    const fetchLotoData = () => Promise.resolve(lotoData);

    // Algorithme optimisé pour générer une grille
    function computeOptimizedGrid(history) {
      const freq = new Uint32Array(49);
      history.forEach(draw => draw.numbers.forEach(num => freq[num - 1]++));
      const selected = new Uint8Array(5);
      let count = 0;
      while (count < 5) {
        const totalWeight = freq.reduce((a, b) => a + (b + 1), 0);
        let rnd = Math.random() * totalWeight, cum = 0;
        for (let i = 0; i < 49; i++) {
          cum += freq[i] + 1;
          if (rnd < cum && ![...selected.subarray(0, count)].includes(i + 1)) {
            selected[count++] = i + 1;
            break;
          }
        }
      }
      selected.sort();
      const chanceFreq = new Uint32Array(10);
      history.forEach(draw => chanceFreq[draw.chance - 1]++);
      const totalChanceWeight = chanceFreq.reduce((a, b) => a + (b + 1), 0);
      let rndChance = Math.random() * totalChanceWeight, cumChance = 0;
      let chance = 1;
      for (let i = 0; i < 10; i++) {
        cumChance += chanceFreq[i] + 1;
        if (rndChance < cumChance) {
          chance = i + 1;
          break;
        }
      }
      return [...selected, chance];
    }

    // Calcul de la prochaine date de tirage avec horaire dynamique :
    // Si c'est un jour de tirage (lundi, mercredi, samedi) et avant 20h10, le tirage est aujourd'hui à 20h20.
    // Sinon, le prochain tirage est affiché avec une heure de 20h10.
    function getNextDrawDate() {
      const now = new Date();
      const drawDays = [1, 3, 6]; // Lundi, Mercredi, Samedi
      const currentDay = now.getDay();
      const currentHours = now.getHours();
      const currentMinutes = now.getMinutes();
      let nextDraw = new Date(now);
      let isDrawDay = drawDays.includes(currentDay);
      if (isDrawDay && (currentHours < 20 || (currentHours === 20 && currentMinutes < 10))) {
        // Aujourd'hui, tirage à 20h20
        nextDraw.setHours(20, 20, 0, 0);
      } else {
        // Chercher le prochain jour de tirage
        let daysToAdd = 1;
        while (!drawDays.includes((currentDay + daysToAdd) % 7)) {
          daysToAdd++;
        }
        nextDraw.setDate(now.getDate() + daysToAdd);
        // Définir l'heure à 20h10 pour le prochain tirage
        nextDraw.setHours(20, 10, 0, 0);
      }
      return nextDraw;
    }

    // Mise à jour du DOM pour les prédictions avec balise <time>
    const predictionEl = document.getElementById("prediction");
    const nextDrawEl = document.getElementById("next-draw-datetime");
    async function getAdvancedPredictions() {
      try {
        const data = await fetchLotoData();
        const nextDrawDate = getNextDrawDate();
        const nextDrawText = nextDrawDate.toLocaleDateString('fr-FR') + ' | ' + nextDrawDate.toLocaleTimeString('fr-FR', {hour: '2-digit', minute:'2-digit'});
        const isoDate = nextDrawDate.toISOString();
        document.getElementById("draw-time").textContent = nextDrawText;
        document.getElementById("draw-time").setAttribute("datetime", isoDate);
        nextDrawEl.innerHTML = `Prochain tirage du Loto (et non l’Euromillions) : <time id="draw-time" datetime="${isoDate}">${nextDrawText}</time>`;
        const grid = computeOptimizedGrid(data.history);
        // Le dernier élément (numéro chance) est entouré de deux étoiles jaunes
        predictionEl.innerHTML = `${grid.slice(0, 5).join(" - ")} - Chance : **<span class="chance-number">${grid[5]}</span>**`;
      } catch (error) {
        console.error("Erreur lors de la génération des prédictions :", error);
        predictionEl.textContent = "Erreur lors du chargement.";
      }
    }
    getAdvancedPredictions();

