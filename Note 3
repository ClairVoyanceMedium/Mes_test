        (function() {
            const currentYear = new Date().getFullYear();
            document.querySelectorAll('[id$="-year"]').forEach(el => {
                el.textContent = currentYear;
            });

            function googleTranslateElementInit() {
                new google.translate.TranslateElement({
                    pageLanguage: 'fr',
                    includedLanguages: 'en,es,de,it,pt,nl,sv,da,fi,no,pl,cs,hu,ro,sk,sl,hr,bg,el,et,lv,lt,ga,mt,ru,zh-CN,hi,ar,bn,ja,ko, urbanos,th,ms,tr,sw',
                    layout: google.translate.TranslateElement.InlineLayout.SIMPLE,
                    autoDisplay: false
                }, 'google_translate_element');
            }

            const cookieConsent = document.getElementById('cookieConsent');
            const acceptCookies = document.getElementById('acceptCookies');
            const rejectCookies = document.getElementById('rejectCookies');
            const videoError = document.getElementById('video-error');
            if (!localStorage.getItem('cookieConsent')) {
                cookieConsent.style.display = 'block';
            }
            acceptCookies.addEventListener('click', () => {
                localStorage.setItem('cookieConsent', 'accepted');
                cookieConsent.style.display = 'none';
                videoError.style.display = 'none';
            });
            rejectCookies.addEventListener('click', () => {
                localStorage.setItem('cookieConsent', 'rejected');
                cookieConsent.style.display = 'none';
                videoError.style.display = 'block';
            });

            const stickyBanner = document.getElementById('stickyBanner');
            const stickyClose = stickyBanner.querySelector('.sticky-close');
            stickyClose.addEventListener('click', () => {
                stickyBanner.style.display = 'none';
            });

            const exitPopup = document.getElementById('exitIntentPopup');
            let popupShown = false;
            document.addEventListener('mouseout', (e) => {
                if (!popupShown && e.relatedTarget === null && e.clientY < 10) {
                    exitPopup.classList.add('active');
                    exitPopup.setAttribute('aria-hidden', 'false');
                    popupShown = true;
                }
            });
            exitPopup.querySelector('.popup-close-button').addEventListener('click', () => {
                exitPopup.classList.remove('active');
                exitPopup.setAttribute('aria-hidden', 'true');
            });

            function updateCarousel(carouselId, direction) {
                const carousel = document.getElementById(carouselId);
                const inner = carousel.querySelector('.carousel-inner');
                const items = inner.querySelectorAll('.carousel-item');
                let current = Array.from(items).findIndex(item => item.classList.contains('active'));
                items[current].classList.remove('active');
                items[current].setAttribute('aria-hidden', 'true');
                items[current].setAttribute('tabindex', '-1');
                current = direction === 'next' ? (current + 1) % items.length : (current - 1 + items.length) % items.length;
                items[current].classList.add('active');
                items[current].setAttribute('aria-hidden', 'false');
                items[current].setAttribute('tabindex', '0');
                carousel.setAttribute('aria-label', `Carrousel, slide ${current + 1} sur ${items.length}`);
            }

            ['avisCarousel', 'blogCarousel'].forEach(id => {
                const carousel = document.getElementById(id);
                carousel.querySelector('.prev').addEventListener('click', () => updateCarousel(id, 'prev'));
                carousel.querySelector('.next').addEventListener('click', () => updateCarousel(id, 'next'));
            });

            const imcForm = document.getElementById('imcForm');
            const imcResult = document.getElementById('imcResult');
            const imcError = document.getElementById('imc-error');
            const imcChart = document.getElementById('imcChart').getContext('2d');
            const imcRecommendation = document.getElementById('imc-recommendation');

            let chartInstance = null;

            imcForm.addEventListener('submit', (e) => {
                e.preventDefault();
                const weight = parseFloat(document.getElementById('weight').value);
                const height = parseFloat(document.getElementById('height').value) / 100;
                imcError.textContent = '';

                if (isNaN(weight) || isNaN(height) || weight < 10 || weight > 300 || height < 0.5 || height > 2.8) {
                    imcError.textContent = 'Veuillez entrer un poids (10-300 kg) et une taille (50-280 cm) valides.';
                    return;
                }

                const imc = (weight / (height * height)).toFixed(1);
                let category = '';
                if (imc < 18.5) category = 'Insuffisance pondérale';
                else if (imc < 25) category = 'Poids normal';
                else if (imc < 30) category = 'Surpoids';
                else category = 'Obésité';

                imcResult.innerHTML = `Votre IMC est <strong>${imc}</strong> (${category}).`;
                imcRecommendation.style.display = 'block';
                imcRecommendation.setAttribute('aria-hidden', 'false');

                if (chartInstance) chartInstance.destroy();
                chartInstance = new Chart(imcChart, {
                    type: 'bar',
                    data: {
                        labels: ['Votre IMC', 'Insuffisance (<18.5)', 'Normal (18.5-24.9)', 'Surpoids (25-29.9)', 'Obésité (≥30)'],
                        datasets: [{
                            label: 'IMC',
                            data: [imc, 18.5, 24.9, 29.9, 30],
                            backgroundColor: ['#87CEEB', '#2ecc71', '#2ecc71', '#f1c40f', '#e74c3c'],
                            borderColor: ['#fff', '#fff', '#fff', '#fff', '#fff'],
                            borderWidth: 1
                        }]
                    },
                    options: {
                        responsive: true,
                        maintainAspectRatio: false,
                        scales: {
                            y: { beginAtZero: true, max: 40 }
                        },
                        plugins: {
                            legend: { display: false },
                            title: { display: true, text: 'Votre IMC comparé aux catégories', font: { size: 16 } }
                        }
                    }
                });
            });

            const audioButtons = document.querySelectorAll('.audio-button');
            audioButtons.forEach(button => {
                button.addEventListener('click', () => {
                    const textIds = button.getAttribute('data-text-id').split(',');
                    const textElements = textIds.map(id => document.getElementById(id) || document.querySelector(id)).filter(el => el);
                    const text = textElements.map(el => el.innerText).join(' ');
                    const utterance = new SpeechSynthesisUtterance(text);
                    utterance.lang = 'fr-FR';
                    utterance.rate = 1;
                    utterance.volume = 1;

                    if (button.classList.contains('playing')) {
                        speechSynthesis.cancel();
                        button.classList.remove('playing');
                    } else {
                        speechSynthesis.cancel();
                        audioButtons.forEach(btn => btn.classList.remove('playing'));
                        speechSynthesis.speak(utterance);
                        button.classList.add('playing');
                    }

                    utterance.onend = () => {
                        button.classList.remove('playing');
                    };
                });
            });

            const videoIframe = document.querySelector('.video-container iframe');
            const muteToggle = document.querySelector('.mute-toggle');
            muteToggle.addEventListener('click', () => {
                const isMuted = videoIframe.src.includes('mute=1');
                videoIframe.src = isMuted ? videoIframe.src.replace('mute=1', 'mute=0') : videoIframe.src.replace('mute=0', 'mute=1');
                muteToggle.textContent = isMuted ? 'Désactiver le son' : 'Activer le son';
            });

            window.dataLayer = window.dataLayer || [];
            function gtag(){dataLayer.push(arguments);}
            gtag('js', new Date());
            gtag('config', 'G-2RSFE6WL4M');

            window.webVitals.getCLS(({ value }) => console.log('CLS:', value));
            window.webVitals.getFID(({ value }) => console.log('FID:', value));
            window.webVitals.getLCP(({ value }) => console.log('LCP:', value));
        })();
    </script>
