        document.querySelectorAll('a[data-tracking-label]').forEach(link => {
            link.addEventListener('click', () => {
                gtag('event', 'click', {
                    'event_category': 'CTA',
                    'event_label': link.getAttribute('data-tracking-label'),
                    'value': 1
                });
            });
