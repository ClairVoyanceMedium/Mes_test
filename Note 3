    <script nonce="xai-premium2025">
        document.getElementById('current-year').textContent = new Date().getFullYear();
        document.getElementById('footer-year').textContent = new Date().getFullYear();

        function googleTranslateElementInit() {
            new google.translate.TranslateElement({
                pageLanguage: 'fr',
                includedLanguages: 'en,es,de,it,pt,nl,sv,da,fi,no,pl,cs,hu,ro,sk,sl,hr,bg,el,et,lv,lt,ga,mt,ru,zh-CN,hi,ar,bn,ja,ko,vi,th,ms,tr,sw',
                layout: google.translate.TranslateElement.InlineLayout.SIMPLE,
                autoDisplay: false
            }, 'google_translate_element');
        }

        const cookieConsent = document.getElementById('cookieConsent');
        const acceptCookies = document.getElementById('acceptCookies');
        const rejectCookies = document.getElementById('rejectCookies');
        if (!localStorage.getItem('cookieConsent')) {
            cookieConsent.style.display = 'block';
        }
        acceptCookies.addEventListener('click', () => {
            localStorage.setItem('cookieConsent', 'accepted');
            cookieConsent.style.display = 'none';
            document.querySelectorAll('.video-container iframe').forEach(iframe => {
                iframe.src = iframe.src.replace('mute=1', 'mute=0');
            });
        });
        rejectCookies.addEventListener('click', () => {
            localStorage.setItem('cookieConsent', 'rejected');
            cookieConsent.style.display = 'none';
            document.querySelectorAll('.video-container iframe').forEach(iframe => {
                document.getElementById('video-error').style.display = 'block';
            });
        });

        const muteToggles = document.querySelectorAll('.mute-toggle');
        muteToggles.forEach(toggle => {
            toggle.addEventListener('click', () => {
                const iframe = toggle.previousElementSibling;
                const isMuted = iframe.src.includes('mute=1');
                iframe.src = isMuted ? iframe.src.replace('mute=1', 'mute=0') : iframe.src.replace('mute=0', 'mute=1');
                toggle.textContent = isMuted ? 'Désactiver le son' : 'Activer le son';
            });
        });

        const audioButtons = document.querySelectorAll('.audio-button');
        let currentAudio = null;
        audioButtons.forEach(button => {
            button.addEventListener('click', () => {
                const textIds = button.getAttribute('data-text-id').split(',');
                let textToRead = '';
                textIds.forEach(id => {
                    const elements = document.querySelectorAll(`#${id}, #${id} p, #${id} h2, #${id} h3, #${id} li, #${id} blockquote`);
                    elements.forEach(el => textToRead += el.textContent + ' ');
                });
                if (currentAudio) {
                    currentAudio.pause();
                    currentAudio = null;
                    audioButtons.forEach(btn => btn.classList.remove('playing'));
                }
                if (!button.classList.contains('playing')) {
                    const utterance = new SpeechSynthesisUtterance(textToRead);
                    utterance.lang = 'fr-FR';
                    utterance.rate = 1;
                    utterance.pitch = 1;
                    window.speechSynthesis.speak(utterance);
                    button.classList.add('playing');
                    currentAudio = utterance;
                    utterance.onend = () => {
                        button.classList.remove('playing');
                        currentAudio = null;
                    };
                }
            });
        });

        const imcForm = document.getElementById('imcForm');
        const imcResult = document.getElementById('imcResult');
        const imcRecommendation = document.getElementById('imcRecommendation');
        const imcError = document.getElementById('imcError');
        const imcChart = document.getElementById('imcChart').getContext('2d');
        let chartInstance = null;
        imcForm.addEventListener('submit', e => {
            e.preventDefault();
            const weight = parseFloat(document.getElementById('weight').value);
            const height = parseFloat(document.getElementById('height').value) / 100;
            if (isNaN(weight) || isNaN(height) || weight < 10 || weight > 300 || height < 0.5 || height > 2.8) {
                imcError.textContent = 'Veuillez entrer un poids (10-300 kg) et une taille (50-280 cm) valides.';
                return;
            }
            imcError.textContent = '';
            const imc = (weight / (height * height)).toFixed(1);
            let category = '';
            if (imc < 18.5) category = 'Insuffisance pondérale';
            else if (imc < 25) category = 'Poids normal';
            else if (imc < 30) category = 'Surpoids';
            else category = 'Obésité';
            imcResult.innerHTML = `Votre IMC : <strong>${imc}</strong> (${category})`;
            imcRecommendation.style.display = 'block';
            imcRecommendation.setAttribute('aria-hidden', 'false');
            if (chartInstance) chartInstance.destroy();
            chartInstance = new Chart(imcChart, {
                type: 'bar',
                data: {
                    labels: ['Votre IMC', 'Insuffisance', 'Normal', 'Surpoids', 'Obésité'],
                    datasets: [{
                        label: 'IMC',
                        data: [imc, 18.5, 25, 30, 40],
                        backgroundColor: ['#87CEEB', '#FFB6C1', '#2ecc71', '#ff6b6b', '#e74c3c'],
                        borderColor: ['#fff'],
                        borderWidth: 1
                    }]
                },
                options: {
                    responsive: true,
                    maintainAspectRatio: false,
                    scales: {
                        y: { beginAtZero: true, max: 50 }
                    },
                    plugins: {
                        legend: { display: false },
                        title: { display: true, text: 'Votre IMC Comparé aux Catégories', font: { size: 16 } }
                    }
                }
            });
        });

        function initCarousel(carouselId, innerId) {
            const carousel = document.getElementById(carouselId);
            const inner = document.getElementById(innerId);
            const items = inner.querySelectorAll('.carousel-item');
            const prev = carousel.querySelector('.prev');
            const next = carousel.querySelector('.next');
            let currentIndex = 0;
            let isTransitioning = false;
            function updateCarousel() {
                if (isTransitioning) return;
                isTransitioning = true;
                items.forEach((item, index) => {
                    item.classList.remove('active');
                    item.setAttribute('aria-hidden', 'true');
                    item.setAttribute('tabindex', '-1');
                    item.style.transform = `translateX(${index < currentIndex ? -100 : 100}%)`;
                    item.style.opacity = '0';
                });
                items[currentIndex].classList.add('active');
                items[currentIndex].setAttribute('aria-hidden', 'false');
                items[currentIndex].setAttribute('tabindex', '0');
                items[currentIndex].style.transform = 'translateX(0)';
                items[currentIndex].style.opacity = '1';
                carousel.setAttribute('aria-label', `Carrousel, slide ${currentIndex + 1} sur ${items.length}`);
                setTimeout(() => { isTransitioning = false; }, 600);
            }
            prev.addEventListener('click', () => {
                if (isTransitioning) return;
                currentIndex = (currentIndex - 1 + items.length) % items.length;
                updateCarousel();
            });
            next.addEventListener('click', () => {
                if (isTransitioning) return;
                currentIndex = (currentIndex + 1) % items.length;
                updateCarousel();
            });
            setInterval(() => {
                currentIndex = (currentIndex + 1) % items.length;
                updateCarousel();
            }, 5000);
            updateCarousel();
        }
        initCarousel('avisCarousel', 'avisCarouselInner');
        initCarousel('blogCarousel', 'blogCarouselInner');

        const popup = document.getElementById('exitIntentPopup');
        const closePopup = document.getElementById('closePopup');
        let hasShownPopup = false;
        document.addEventListener('mouseout', e => {
            if (!hasShownPopup && e.relatedTarget === null && e.clientY <= 0) {
                if (localStorage.getItem('cookieConsent') === 'accepted') {
                    popup.style.display = 'block';
                    hasShownPopup = true;
                }
            }
        });
        closePopup.addEventListener('click', () => {
            popup.style.display = 'none';
        });

        window.dataLayer = window.dataLayer || [];
        function gtag(){dataLayer.push(arguments);}
        gtag('js', new Date());
        gtag('config', 'G-2RSFE6WL4M');

        document.querySelectorAll('a[data-tracking-label]').forEach(link => {
            link.addEventListener('click', () => {
                gtag('event', 'click', {
                    'event_category': 'CTA',
                    'event_label': link.getAttribute('data-tracking-label'),
                    'value': 1
                });
            });
        });
    </script>
